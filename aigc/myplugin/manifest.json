{
  "manifest_version": 3,
  "name": "MyPlugin",
  "version": "1.0",
  "description": "一个用于切换网页背景颜色的Chrome插件",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon48.png"
    }
  },
  "icons": {
    "48": "icons/icon48.png"
  }
}